          [
            "Ne mabumba a mwiulu",
            "Nenu mupopwelai;",
            "Mwimbenga nabo Iwimbo wa myaka",
            "Ne kumuvwika Aye."
          ],
          [
            "Anweba basalululwa",
            "Bo akula Yesu,",
            "Mutoteshai wimupulusha,",
            "Mumuvwike bya Mfumu."
          ]
        ],
        "chorus": [
             "Muvwikai byonse bya bufumu,",
            "Ekale Mfumu wa bonse."
        ],
