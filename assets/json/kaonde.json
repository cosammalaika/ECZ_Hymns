      "author": "Unknown",
      "meter": "8.7.8.7.D.",
      "tuneName": "Unknown",
      "verses": [
        [
          "ANWE bilengwa byonsetu,",
          "Mwitutoteshepo Lesa,",
          "Aleluya, Aleluya.",
          "Anwe juba mwasamisha, Ne anwe gondo mwakema,",
          "Mumutote, mumutote,",
          "Aleluya, Aleluya, Aleluya."
        ],
        [
          "Anwe mwela mupupisha,",
          "Anwe makumbi mwiulu,",
          "Mumutote, Aleluya.",
          "Anwe ntanda musekele, Anwe minyanya mutote,",
          "Mumutote, mumutote,",
          "Aleluya, Aleluya, Aleluya."
        ],
        [
          "Ne baNesa manimimbile,",
          "Mumutote, Aleluya.",
          "Ne anweba ba bulanda, Mumutwike'bo Yehoba.",
          "Mumutote, mumutote,",
          "Aleluya, Aleluya, Aleluya."
        ],
        [
          "Byonse bitote Mulenga,",
          "Ne kumwalanena panshi,",
          "Mumutote, Aleluya.",
          "Mutote Tata ne Mwana, Ne Mupashi, Lesa umo,",
          "Mumutote, mumutote,",
          "Aleluya, Aleluya, Aleluya."
        ]
      ],
      "chorus": [],
      "addedChorus": []
    },
    {
      "id": 2,
      "title": "ATOTWE Nambo wabulume",
      "author": "Unknown",
      "meter": "8.7.8.7.D.",
      "tuneName": "Unknown",
      "verses": [
        [
          "ATOTWE Nkambo wabulume,",
          "Mfumu wa byonse;",
          "Ami mwine namutote mambo wa-mpulusha;",
          "Anwe bonse, mwiye mu kumutota",
          "Na jiwi ja lusekelo."
        ],
        [
          "Atotwe Nkambo, wibikazha byonse bulongo;",
          "Ye wimushinjila bibi, ne kwimu-lamwina.",
          "Ke mwayukanyi? Byonse bimu-fwainwa",
          "Bemumwena'byo kwi Aye."
        ],
        [
          "Atotwe Nkambo, ulumbulula byenu byonse;",
          "Kine mumona bibusa byanji moba onse.",
          "Langulukai byakonsha kuba Aye,",
          "Wimubila lusa lonse."
        ],
        [
          "Atotwe Nkambo! Namunemekeshetu kine,",
          "Byonse bipema, nenuwa, mumu totesh' Aye.",
          "Banji bonse, bamwimbile bulongo",
          "Na nyimbo ya kumutota."
        ]
      ],
      "chorus": [],
      "addedChorus": []
    },
    {
      "id": 3,
      "title": "BANTU bonse pano panshi",
      "author": "Unknown",
      "meter": "8.7.8.7.D.",
      "tuneName": "Unknown",
      "verses": [
        [
          "BANTU bonse pano panshi",
          "Bamwimbile Nkambo yetu,",
          "Bamwingijile na moyo,",
          "Bapwile na kumutota."
        ],
        [
          "Nkambo, ye Lesa wa kine,",
          "Ye wibalengele bonse;",
          "Yetu mikoko ya kwanji,",
          "Ye witulama atweba."
        ],
        [
          "Nanchi tutwele mwazhila,",
          "Sa tumutota na nyimbo;",
          "Jizhina janji jitotwe,",
          "Jitumbalale konsetu."
        ],
        [
          "Tufwenye na lusekelo",
          "Konka kuji Aye mwine;",
          "Tumunemeke Mukuzhi,",
          "Kyo kitufwainw'atweba."
        ],
        [
          "Mambo Nkambo ye wawama,",
          "Lusa wanji lo lwa myaka:",
          "Byambo byanji byo bya kine,",
          "Bikekalanga pe ne pe."
        ]
      ],
      "chorus": [],
      "addedChorus": []
    },
    {
      "id": 4,
      "title": "BYO bwakya ke lukelo",
      "author": "Unknown",
      "meter": "8.7.8.7.D.",
      "tuneName": "Unknown",
      "verses": [
        [
          "BYO bwakya ke lukelo",
          "Nasekela namba, Yesu atotwe pe.",
          "Mu bimye byonsetu",
          "Nafwenya kwi Aye,",
          "Yesu atotwe pe."
        ],
        [
          "Nemwimbile Anweba,",
          "Lesa ye natemwa,",
          "Yesu atotwe pe.",
          "Lwimbo wami uno",
          "Ye wa moba onse",
          "Yesu atotwe pe."
        ],
        [
          "Byo numvwa lusengo",
          "Wa kungita kulomba, Yesu atotwe pe.",
          "Umvwai bakwetu",
          "Jiwi ja lusengo,",
          "Yesu atotwe pe."
        ],
        [
          "Kechi nkakoka kwimba",
          "Lwimbo ne bakwetu,",
          "Yesu atotwe pe.",
          "Ye wa lusekelo",
          "Lwa moba onsetu,",
          "Yesu atotwe pe."
        ],
        [
          "Byo namona bya malwa",
          "Kino kyansekesha,",
          "Yesu atotwe pe.",
          "Sa nji na bulanda",
          "Jino jankosesha, Yesu atotwe pe."
        ],
        [
          "Byo ndanguluka bibi",
          "Jino jindamwina, Yesu atotwe pe.",
          "Ba mfishi bachina",
          "Pa kwijumvwa jino,",
          "Yesu atotwe pe."
        ],
        [
          "Byo ndalatu bufuku",
          "Ne tulo kwiya ne",
          "Yesu atotwe pe.",
          "Bwaluka ke mute",
          "Inge twimba uno",
          "Yesu atotwe pe."
        ],
        [
          "Uno Iwimbo wa myaka",
          "Wa kutota Nkambo,",
          "Yesu atotwe pe.",
          "Mu myaka yonsetu",
          "Ye tukembangatu",
          "Yesu atotwe pe."
        ]
      ],
      "chorus": [],
      "addedChorus": []
    },
    {
        "id": 5,
        "title": "JITOTWE ja Yesu Kilishitu",
        "author": "Unknown",
        "meter": "8.7.8.7.D.",
        "tuneName": "Unknown",
        "verses": [
          [
            "JITOTWE ja Yesu Kilishitu,",
            "Jitotwe ku bonse,",
            "Letai munchinya wa bufumu",
            "Ne kumuvwika Aye."
           
          ],
          [
            "Ne ba mu mikoka yonse",
            "Ya pano pa ntanda,",
            "Munemekai ne kumutota",
            "Mumuvwike yenkaye."
